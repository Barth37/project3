{
  "name": "cookbook",
  "version": "1.0.0",
  "description": "recipe finder app",
  "main": "server.js",
  "scripts": {
    "start:old": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd cookbook && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd cookbook && npm install",
    "build": "cd cookbook && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Barth37/project3.git"
  },
  "author": "crystal, dominique, jake, brian and dj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Barth37/project3/issues"
  },
  "homepage": "https://github.com/Barth37/project3#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "connect-flash": "^0.1.1",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.16.2",
    "if-env": "^1.0.4",
    "mongojs": "^2.6.0",
    "mongoose": "^5.6.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
